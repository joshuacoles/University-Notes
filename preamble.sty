% Unit Vectors
\newcommand\uvec[1]{\hat{#1}}

\newcommand\uvi{\mathbf{i}}
\newcommand\uvj{\mathbf{j}}
\newcommand\uvk{\mathbf{k}}

% Helpers

\newcommand{\'}{\text}

\newcommand\sub{\subseteq}
\newcommand\setp[2]{\set{ {#1} \mathrel{|} {#2} }}

\newcommand\({\left(}
\newcommand\){\right)}

\newcommand{\cp}{\times}
\newcommand{\dp}{\cdot}

\newcommand\d{\mathrm{d}}
\newcommand\rd{\mathrm{d}}

\newcommand\parfrac[2]{\frac{\partial #1}{\partial #2}}
\newcommand\dparfrac[2]{\dfrac{\partial #1}{\partial #2}}

\newcommand\epsi{\epsilon}
\newcommand\eps{\varepsilon}

\newcommand{\norm}[1]{\left\|#1\right\|}

\newcommand {\lst }[3]{#1_{#2},\dots ,#1_{#3}}

\newcommand\intl{\int\limits}


% Vec Calc
\newcommand{\nab}{\nabla}
\newcommand{\divrg}{\nab \cdot}
\newcommand{\curl}{\nab \cp}

% Physics

\newcommand\angstrom{\overset{\lower.5em\circ}{\mathrm{A}}}

%% EM

\newcommand\F{\vec{F}}
\newcommand\E{\vec{E}}
\newcommand\B{\vec{B}}
\newcommand\r{\vec{r}}
\newcommand\J{\vec{J}}

%% CMP

